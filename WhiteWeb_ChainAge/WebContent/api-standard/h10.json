{
	"url": "http://data.chainage.jp/caweb/cc/currencyApiController.do?exchangeMarketInfo",
	"datatype": "txt",
	"arguments": [
		{
			"name": "id",
			"value": "?",
			"desc": "交易所id"
		}
	],
	"schema": [
		{
			"name": "markInfoList.numNarketPairs",
			"value": "int",
			"desc": "市场数量"
		},
		{
			"name": "markInfoList.marketPair",
			"value": "string",
			"desc": "配对市场名称"
		},
		{
			"name": "markInfoList.marketPair",
			"value": "string",
			"desc": "配对市场名称"
		},
		{
			"name": "markInfoList.marketPairBaseSymbol",
			"value": "string",
			"desc": "基准货币符号"
